    private void b1ActionPerformed(java.awt.event.ActionEvent evt) {                                   
        if (r1.isSelected())
        global.weapon=((String)c1.getSelectedItem());
        {
            if (r2.isSelected())
            global.weapon=((String)c2.getSelectedItem());
        }
        {
            if(r3.isSelected())
            global.weapon=((String)c3.getSelectedItem());
        }
        {
            if(r4.isSelected())
            global.weapon=((String)c4.getSelectedItem());
        }
        {
            global.Cost=txt1.getText();

        }
        this.dispose();
        new C3().setVisible(true);
        // TODO add your handling code here:
    }                                  

    private void jButton2ActionPerformed(java.awt.event.ActionEvent evt) {                                         
        b1.setVisible(true);
        int a=c1.getSelectedIndex();

        int c; double e,f,g=0 ;
        int d;
        String b;
        if(r1.isSelected())
        {
            if(a==0){
                c=2200;
                txt1.setText("Rupees "+c);
                d=22;
                txt2.setText(d+" per Km");
                e=20000.00;
                txt3.setText(e+" Kms");
            }
            if(a==1){
                c=1500;
                txt1.setText("Rupees "+c);
                d=27;
                txt2.setText(d+" per Km");
                e=22000.00;
                txt3.setText(e+" Kms");
            }
        }
        int h=c2.getSelectedIndex();
        if(r2.isSelected())
        {
            if(h==0){
                c=2100;
                txt1.setText("Rupees "+c);
                d=20;
                txt2.setText(d+" per Km");
                e=24000.50;
                txt3.setText(e+" Kms");
            }
            if(h==1){
                c=1700;
                txt1.setText("Rupees "+c);
                d=27;
                txt2.setText(d+" per Km");
                e=26000.47;
                txt3.setText(e+" Kms");
            }
            if(h==2){
                c=1600;
                txt1.setText("Rupees"+c);
                d=16;
                txt2.setText(d+" per Km");
                e=29000.99;
                txt3.setText(e+" Kms");
            }
        }
        int i=c3.getSelectedIndex();
        if(r3.isSelected())
        {
            if(i==0){
                c=2370;
                txt1.setText("Rupees"+c);
                d=20;
                txt2.setText(d+" per Km");
                e=343420.40;
                txt3.setText(e+" Kms");
            }
            if(i==1){
                c=2670;
                txt1.setText("Rupees "+c);
                d=27;
                txt2.setText(d+" per Km");
                e=345070.780;
                txt3.setText(e+" Kms");
            }
            if(i==2){
                c=2230;
                txt1.setText("Rupees "+c);
                d=24;
                txt2.setText(d+" /Km");
                e=4304500.780;
                txt3.setText(e+" Kms");
            }
        }
        int j=c4.getSelectedIndex();
        if(r4.isSelected())
        {
            if(j==0){
                c=2150;
                txt1.setText("Rupees "+c);
                d=21;
                txt2.setText(d+" per Km");
                e=3456000.79;
                txt3.setText(e+" Kms");
            }
            if(j==1){
                c=2347;
                txt1.setText("Rupees "+c);
                d=27;
                txt2.setText(d+" per Km");
                e=78900.760;
                txt3.setText(e+" Kms");
            }
            if(j==2){
                c=2410;
                txt1.setText("Rupees "+c);
                d=24;
                txt2.setText(d+" per Km");
                e=6545009.87;
                txt3.setText(e+" Kms");
            }
        }

